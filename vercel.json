{
    "version": 2,
    "builds":[
        {
            "src": "poll/wsgi.py" ,
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb", "runtime": "python3.9"}
        }
    ],
    "routes":[
        {

            "src": "/static/(.*)",
            "dest": "/static/$l"
        },
        {

            "src":"/(.*)",
            "dest": "poll/wsgi.py"
        }
    ]
}
